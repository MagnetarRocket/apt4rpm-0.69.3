	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(apt4rpm_libSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(apt4rpm_libdir)/$$f'"; \
	    $(apt4rpm_libSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(apt4rpm_libdir)/$$f"; \
	  else :; fi; \
	done

